{
    // 设置什么范围内点(dot)为trigger, 触发补全框
    "auto_complete_triggers":
    [{
        "selector": "source.python - string - comment - constant.numeric",
        "characters": "."
    }],
    //设置触发补全的范围
    "auto_complete_selector": "source, text",
    // snippet中是否显示补全
    "auto_complete_with_fields": true,
    // 小地图一直显示当前位置, 默认鼠标移上去显示
    "always_show_minimap_viewport": true,
    // tab提交, false时会出现tab上屏后继续补全而不是缩进
    "auto_complete_commit_on_tab": true,
    // 补全到末尾循环到开头, 默认到末尾停止补全
    "auto_complete_cycle": true,
    // 文件夹标签加粗
    "bold_folder_labels": true,
    // 光标样式
    "caret_style": "phase",
    "color_scheme": "Packages/Colorcoder/Monokai Extended (Colorcoded).tmTheme",
    "default_line_ending": "unix",
    "detect_slow_plugins": false,
    // 设置缩进线的样式
    "indent_guide_options": ["draw_active"],
    "draw_white_space": "all",
    "ensure_newline_at_eof_on_save": true,
    "file_exclude_patterns":
    [
        "*.pid",
        "*.pyc"
    ],
    "folder_exclude_patterns":
    [
        ".idea",
        ".git",
        "__pycache__"
    ],
    "font_face": "consolas",
    // ?
    "font_options":
    [
        "subpixel_antialias",
        "no_bold"
    ],
    "font_size": 11.0,
    "highlight_line": true,
    // 高亮修改过的标签
    "highlight_modified_tabs": true,
    "ignored_packages":
    [
        "",
        "Markdown",
        "MarkdownBuild",
        "SublimeCodeIntel"
    ],
    "line_padding_bottom": 1,
    "line_padding_top": 1,
    "original_color_scheme": "Packages/Monokai Extended/Monokai Extended.tmTheme",
    "rulers":
    [
        72,
        79
    ],
    "show_full_path": true,
    "tab_size": 4,
    "translate_tabs_to_spaces": true,
    "trim_trailing_white_space_on_save": true,
    "word_wrap": true,
    "wrap_width": 80,
    // vintageous 设置目前配置都写在user的settings里
    // 切屏不重置模式
    "vintageous_reset_mode_when_switching_tabs": false
}
